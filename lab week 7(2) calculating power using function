#include<stdio.h>
#include<math.h>
void power(int,int);
int main()
{
    int x,y;
    printf("enter a number");
    scanf("%d",&x);
     printf("enter a number");
    scanf("%d",&y);
    power(x,y);
    
    return 0;
}
void power(int x,int y)
{
    int a;
    a=pow(x,y);
    printf("%d",a);
    
}
